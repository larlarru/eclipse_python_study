<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>WEB2</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>MyCrawl</display-name>
    <servlet-name>MyCrawl</servlet-name>
    <servlet-class>kr.co.ncs.web.MyCrawl</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>MyCrawl</servlet-name>
    <url-pattern>/mycrawl</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>MyLogin</display-name>
    <servlet-name>MyLogin</servlet-name>
    <servlet-class>kr.co.ncs.web.MyLogin</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>MyLogin</servlet-name>
    <url-pattern>/mylogin</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>MyLogout</display-name>
    <servlet-name>MyLogout</servlet-name>
    <servlet-class>kr.co.ncs.web.MyLogout</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>MyLogout</servlet-name>
    <url-pattern>/mylogout</url-pattern>
  </servlet-mapping>
</web-app>